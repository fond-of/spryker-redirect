  "require": {
    "php": ">=7.3",
    "jaybizzle/crawler-detect": "^1.2.0",
    "sinergi/browser-detector": "^6.1.0",
    "spryker/store": "^1.12.0",
    "spryker/session": "^4.4.0",
    "spryker/application": "^3.18.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*",
    "spryker/silex": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/",
      "Orm\\Zed\\": "src/Orm/Zed/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpro/grumphp": true
    }
  }
}
